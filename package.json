{
  "name": "vinberodb-http-adapter",
  "version": "1.1.1",
  "description": "An HTTP adapter for VinberoDB",
  "main": "build/index.js",
  "scripts": {
    "build": "rm -rf build && babel src --out-dir build --ignore tests/",
    "lint": "eslint src",
    "prepublishOnly": "npm run build",
    "test": "mocha -r babel-register --recursive src/tests --timeout 5000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oprogramador/vinberodb-http-adapter.git"
  },
  "keywords": [
    "http",
    "adapter",
    "vinbero",
    "vinberodb"
  ],
  "author": "oprogramador",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oprogramador/vinberodb-http-adapter/issues"
  },
  "homepage": "https://github.com/oprogramador/vinberodb-http-adapter#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-register": "^6.24.0",
    "body-parser": "^1.18.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-subset": "^1.5.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-improved": "^2.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "express": "^4.16.2",
    "mocha": "^3.2.0",
    "sinon": "^2.1.0",
    "sinon-as-promised": "^4.0.3",
    "sinon-chai": "^2.9.0",
    "vinberodb-test-helpers": "^1.1.5"
